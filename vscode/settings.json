{
  "github.copilot.editor.enableAutoCompletions": true,
  "workbench.sideBar.location": "right",
  "explorer.confirmDelete": false,
  "breadcrumbs.enabled": false,
  "editor.rulers": [
    {
      "column": 80,
      "color": "#333333"
    },
    {
      "column": 120,
      "color": "#333333"
    }
  ],
  "editor.folding": false,
  "editor.lineNumbers": "on",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.showTabs": "none",
  "editor.minimap.enabled": false,
  "terminal.integrated.defaultLocation": "editor",
  "workbench.panel.defaultLocation": "bottom",
  "workbench.panel.opensMaximized": "never",
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.showLinkHover": false,
  "terminal.integrated.scrollback": 100000,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.git": true
  },
  "editor.guides.bracketPairs": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualstar": true,
  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
  "vim.statusBarColors.insert": "#BF616A",
  "vim.statusBarColors.visual": "#B48EAD",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.insertModeKeyBindings": [],
  "vim.normalModeKeyBindings": [
    {
      "before": ["s", "s"],
      "after": ["<leader>", "<leader>", "2", "s", "<char>", "<char>"]
    },
    {
      "before": [",", "c"],
      "after": ["g", "c"]
    }
  ],
  "editor.tabSize": 2,
  "terminal.integrated.tabStopWidth": 2,
  "vim.visualModeKeyBindings": [
    {
      "before": [",", "c"],
      "after": ["g", "c"]
    }
  ],
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "jest.runMode": "on-demand",
  "git.openRepositoryInParentFolders": "never",
  "workbench.startupEditor": "readme",
  "workbench.editor.tabSizing": "shrink",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Default High Contrast",
  "git.ignoreMissingGitWarning": true,
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "'Bizin Gothic Discord', Menlo, Monaco, 'Courier New', monospace",
  "window.zoomLevel": 2,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "[javascript]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
